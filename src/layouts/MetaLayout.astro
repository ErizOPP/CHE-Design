---
interface Props {
title: string;
titleHeader: string;
metaDescription: string;
}

const { title, titleHeader, metaDescription } = Astro.props;
---       

        <meta charset="UTF-8" />
        <meta name="description" content={metaDescription}>
        <meta  content={metaDescription}>
        <meta name="viewport" content="width=device-width" />
        <meta name="generator" content={Astro.generator} />

        <meta
        property="og:image"
        content="/simboloche.svg" />
        <meta name="author" content="CHE! design">
        <meta name="keywords" content="diseño gráfico, diseño, logo, marca, branding, editorial, social media, community manager, flyer, banner, cartel, publicidad, website, pagina web">


        <meta content={title} property="og:title" />
        <meta content={title} property="twitter:title" />
        <meta name="twitter:card" content="summary_large_image" />